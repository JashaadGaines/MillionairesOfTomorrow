  margin: 0 auto

.imageBack
  height 400px
  background-image url('http://humbleinsights.files.wordpress.com/2013/03/journey.jpg')
  background-size cover
  color white

.imageBack #quote
    position absolute;
    top 40%
    text-align right
    right 10%
    font-family ":Lucida Sans Unicode"


.blur
  opacity 0
  height: 100%
  background-image url('https://d262ilb51hltx0.cloudfront.net/fit/c/1600/1280/gradv/29/81/40/darken/50/blur/50/1*4ncz3hLxmL8E_bUh-0z62w.jpeg')


nav li
  display: inline
  top: 30px;
  margin: auto 10px
  line-height: 2
  height relative
  a
    color white
    text-decoration: none;

nav li:first-child
  content: ""
  border-right: 1px solid #0000FF



nav ul
  margin: auto
  min-height 40px
  background #303030













